#include<stdio.h>
#define n 5
int f=-1,r=-1;
int q[n];
void enqueue(int data)
{
    if(r==n-1)
    {
        printf("queue full");
        return;
    }
    if(f==-1 && r==-1)
    {
        f=r=0;
        q[r]=data;
        return;
    }
    q[++r]=data;
}
void dequeue()
{
    if(f==-1 && r==-1)
    {
        printf("empty");
        return;
    }
    printf("%d ",q[f]);
    f++;
    if(f>r)
    {
        f=r=-1;
    }
}
void peek()
{
    if(f==-1 && r==-1)
    {
        printf("unavailable");
        return;
    }
    printf("%d ",q[f]);
}
int main()
{
    enqueue(10);
    enqueue(20);
    enqueue(30);
    enqueue(40);
    enqueue(50);
    enqueue(60);
    dequeue();
    peek();
}
