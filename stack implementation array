#include<stdio.h>
#define n 5
int st[n];
int top=-1;
void push(int data)
{
    if(top==n-1)
    {
        printf("stack is full");
        return;
    }
    st[++top]=data;
}
void pop()
{
    if(top==-1)
    {
        printf("stack is empty");
        return;
    }
    printf("%d",st[top--]);
}
void peek()
{
    if(top==-1)
    {
        printf("unavailable");
        return;
    }
    printf("%d",st[top]);
}
int main()
{
    push(10);
    push(20);
    push(30);
    push(40);
    push(50);
    pop();
    printf("\n");
    peek();
}
