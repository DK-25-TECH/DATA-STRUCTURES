#include<stdio.h>
#include<stdlib.h>
typedef struct node{
    int data;
    struct node *next;
    struct node *prv;
}node;

node* create(int data)
{
    node *nn=malloc(sizeof(struct node));
    nn->data=data;
    nn->next=NULL;
    nn->prv=NULL;
    return nn;
}

void insLast(node **head,int data)
{
    node *nn=create(data);
    if(*head==NULL)
    {
        nn->next=*head;
        *head=nn;
        return;
    }
    node *tp=*head;
    while(tp->next!=NULL)
    {
        tp=tp->next;
    }
    tp->next=nn;
    nn->prv=tp;
}

void deleteDup(node **head1)
{
    node *tp=*head1;
    while(tp!=NULL)
    {
        node *x=tp->next;
        node *prv=tp;
        while(x!=NULL)
        {
             if(tp->data==x->data)
              {
                  prv->next=x->next;
                  if(x->next!=NULL)
                  {
                      x->next->prv=prv;
                  }
                  free(x);
                  x=prv->next;
              }
              else
              {
                  prv=x;
                  x=x->next;
              }
        }
        tp=tp->next;
              
    }
}
void display(node *head1)
{
    node *tp=head1;
    while(tp!=NULL)
    {
        printf("%d ",tp->data);
        tp=tp->next;
    }
}


int main()
{
    node *head1=NULL;
    
    insLast(&head1,1);
    insLast(&head1,2);
    insLast(&head1,1);
    insLast(&head1,4);
    insLast(&head1,2);
    insLast(&head1,2);
    insLast(&head1,1);

   
    display(head1);
    deleteDup(&head1);
    printf("\n");
    display(head1);
    
    
}






