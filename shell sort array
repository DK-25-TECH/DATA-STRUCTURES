#include<stdio.h>
int check(int a,int b)
{
    return a>b;
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
    printf("\n");
    
    for(int gap=n/2;gap>0;gap=gap/2)
    {
        for(int j=gap;j<n;j++)
        {
            for(int i=(j-gap);i>=0;i=i-gap)
            {
                if(check(a[i],a[j]))
                {
                    int x=a[i];
                    a[i]=a[j];
                    a[j]=x;
                }
            }
        }
    }
    for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
}
