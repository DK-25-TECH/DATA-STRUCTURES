#include<stdio.h>

void sort(int a[],int n)
{
   int x=n/2;
   for(int gap=x;gap>0;gap/=2)
   {
       for(int j=gap;j<n;j++)
       {
           int key=a[j];
           int i=j-gap;
           while(i>=0 && a[i]>key)
           {
               a[i+gap]=a[i];
               i-=gap;
           }
           a[i+gap]=key;
       }
   }
}
void print(int a[],int n)
{
    for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    print(a,n);
    printf("\n");
    sort(a,n);
    printf("\n");
    print(a,n);
    
}
