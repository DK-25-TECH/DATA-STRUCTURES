
#include <stdio.h>
void display(int a[],int n)
{
    for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
}
void sort(int a[],int n)
{
    for(int i=1;i<n;i++)
    {
       int j=i-1;
       int x=a[i];
       while(j>=0 && a[j]>x)
       {
           a[j+1]=a[j];
           j--;
       }
       a[j+1]=x;
    }
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    display(a,n);
    printf("\n");
    sort(a,n);
    printf("\n");
    display(a,n);
    
    
}
