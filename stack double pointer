#include<stdio.h>
#include<stdlib.h>

typedef struct node
{
    int data;
    struct node* next;
}node;

node* create(int data)
{
    node *nn=malloc(sizeof(struct node));
    nn->data=data;
    nn->next=NULL;
    return nn;
}

void push(node **top,int data)
{
    node *nn=create(data);
    if(*top==NULL)
    {
        *top=nn;
        return;
    }
    nn->next=*top;
    *top=nn;
}

void pop(node **top)
{
    if(*top==NULL)
    {
        printf("empty");
        return;
    }
    node *tp=*top;
    
    
    *top=(*top)->next;
    free(tp);
}
void display(node *top)
{
    if(top==NULL)
    {
        return;
    }
    node *tp=top;
    while(tp!=NULL)
    {
        printf("%d ",tp->data);
        tp=tp->next;
    }
}

int main()
{
    node *top=NULL;
    push(&top,5);
    push(&top,15);
    push(&top,25);
    push(&top,35);
    push(&top,45);
    display(top);
    pop(&top);
    printf("\n");
    display(top);
}
