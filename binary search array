#include<stdio.h>
void display(int a[],int n)
{
     for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
}

int binary(int a[],int n,int key)
{
    int l=0,r=n;
    while(l<=r)
    {
        int mid=(r-l)/2;
        if(a[mid]==key)
        {
            return mid;
        }
        else if(a[mid]>key)
        {
            r=mid-1;
        }
        else
        {
            l=mid+1;
        }
    }
    return -1;
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    display(a,n);
    int x=binary(a,n-1,5);
    printf("%d",x);
   
}
