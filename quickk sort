
#include<stdio.h>

void print(int a[],int n)
{
    for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
}
void swap(int *a,int *b)
{
    int x=*a;
    *a=*b;
    *b=x;
}
int partition(int a[],int l, int r)
{
    int px=a[l];
    int start=l+1;
    int end=r;
    while(start<end)
    {
       while(a[start]<=px && start<r)
       {
           start++;
       }
       while(a[end]>px && end>l)
       {
           end--;
       }
       if(start<end)
       {
           swap(&a[start],&a[end]);
       }
    }
    swap(&a[l],&a[end]);
    return end;
    
}
void sort(int a[],int l,int r)
{
    if(l<r)
    {
        int x=partition(a,l,r);
        sort(a,l,x-1);
        sort(a,x+1,r);
    }
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    print(a,n);
    printf("\n");
    sort(a,0,n-1);
    printf("\n");
    print(a,n);
    
}
