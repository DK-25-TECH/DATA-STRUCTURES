#include<stdio.h>
#include<stdlib.h>
typedef struct node{
    int data;
    struct node* next;
}node;

node* create(int data)
{
    node *nn=malloc(sizeof(struct node));
    nn->data=data;
    nn->next=NULL;
    return nn;
}

void enqueue(node ** front,node ** rear,int data)
{
    node *nn=create(data);
    if(*rear==NULL)
    {
        *front=nn;
        *rear=nn;
        return;
    }
    (*rear)->next=nn;
    *rear=nn;
}
void dequeue(node **front,node **rear)
{
    if(*front==NULL)
    {
        printf("empty");
        return;
    }
    node *x=*front;
    printf("%d ",x->data);
    *front=x->next;
    free(x);
}
void display(node *front,node *rear)
{
    node *x=front;
    while(x != NULL)
    {
        printf("%d ",x->data);
        x=x->next;
    }
}

int main()
{
    node *front=NULL,*rear=NULL;
    enqueue(&front,&rear,1);
    enqueue(&front,&rear,2);
    enqueue(&front,&rear,3);
    enqueue(&front,&rear,4);
    enqueue(&front,&rear,5);
    display(front,rear);
    printf("\n");
    dequeue(&front,&rear);
}
