#include<stdio.h>
void display(int a[],int n)
{
     for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
}
void swap(int *a,int *b)
{
    int x=*a;
    *a=*b;
    *b=x;
}
int partition(int a[],int l,int r)
{
    int pivot=a[l];
    int start=l;
    int end=r;
    while(start<end)
    {
        while(start<=r && a[start]<=pivot)
        {
            start++;
        }
        while(end>=l && a[end]>pivot)
        {
            end--;
        }
        if(start<end)
        {
            swap(&a[start],&a[end]);
        }
    }
    swap(&a[l],&a[end]);
    return end;
}
void quick(int a[],int l,int r)
{
    if(l<r)
    {
        int x=partition(a,l,r);
        quick(a,l,x-1);
        quick(a,x+1,r);
    }
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    display(a,n);
    quick(a,0,n-1);
    printf("\n");
    display(a,n);
   
}
