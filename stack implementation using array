#include<stdio.h>
#define n 10
int st[n];
int  top=-1;
void push(int data)
{
    
    if(top==n-1)
    {
        printf("stack is full return");
        return;
    }
    st[++top]=data;
}
int pop()
{
    if(top==-1)
    {
        printf("stack is empty");
        return -1;
    }
    return st[top--];
}

void display()
{
    if(top==-1)
    {
        printf("stack empty");
        return;
    }
    for(int i=0;i<=top;i++)
    {
        printf("%d ",st[i]);
    }
}
int main()
{
    for(int i=0;i<n;i++)
    {
        int x;
        scanf("%d ",&x);
        push(x);
    }
    pop();
    display();
}










