#include<stdio.h>
void display(int a[],int n)
{
     for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
}

int binary(int a[],int l,int r,int key)
{
   
    if(l>r)
    {
        return -1;
    }
        int mid=l+(r-l)/2;
        if(a[mid]==key)
        {
            return mid;
        }
        else if(a[mid]>key)
        {
           binary(a,l,mid-1,key);
        }
        else
        {
            binary(a,mid+1,r,key);
        }
    
    return -1;
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    display(a,n);
    int x=binary(a,0,n-1,5);
    printf("%d",x);
   
}
