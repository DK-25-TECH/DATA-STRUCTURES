#include<stdio.h>

void sort(int a[],int n)
{
    for(int i=0;i<n-1;i++)
    {
        int x=i;
        for(int j=i+1;j<n;j++)
        {
            if(a[j]<a[x])
            {
                x=j;
            }
        }
        int z=a[i];
        a[i]=a[x];
        a[x]=z;
    }
}
void print(int a[],int n)
{
    for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    print(a,n);
    printf("\n");
    sort(a,n);
    printf("\n");
    print(a,n);
    
}
