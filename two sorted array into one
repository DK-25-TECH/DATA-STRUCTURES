#include<stdio.h>
void merge(int a[],int b[],int n,int c[])
{
    int i=0,j=0;
    int x=0;
    while(i<n && j<n)
    {
        if(a[i]<b[j])
        {
            c[x++]=a[i++];
        }
        else
        {
            c[x++]=b[j++];
        }
    }
    while(i<n)
    {
        c[x++]=a[i++];
    }
    while(j<n)
    {
        c[x++]=b[j++];
    }
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    int b[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++)
    {
        scanf("%d",&b[i]);
    }
    int c[n+n];
    
    merge(a,b,n,c);
    for(int i=0;i<n+n;i++)
    { 
        printf("%d ",c[i]);
    }
}
