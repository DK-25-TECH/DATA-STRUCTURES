#include<stdio.h>
#include<stdlib.h>
typedef struct node{
    int data;
    struct node *next;
}node;

node* create(int data)
{
    node *nn=malloc(sizeof(struct node));
    nn->data=data;
    nn->next=NULL;
    return nn;
}

void insLast(node **head,int data)
{
    node *nn=create(data);
    if(*head==NULL)
    {
        nn->next=*head;
        *head=nn;
        return;
    }
    node *tp=*head;
    while(tp->next!=NULL)
    {
        tp=tp->next;
    }
    tp->next=nn;
}

void display(node *head)
{
    node *tp=head;
    while(tp!=NULL)
    {
        printf("%d ",tp->data);
        tp=tp->next;
    }
}

int isPresent(node *x,int data)
{
    while(x!=NULL)
    {
        if(x->data==data)
        {
            return 1;
        }
        x=x->next;
    }
    return 0;
}

node *unionList(node *head1,node *head2)
{
    node *uni=NULL;
    node *x=head1;
    while(x!=NULL)
    {
        if(!isPresent(uni,x->data))
        {
            insLast(&uni,x->data);
        }
        x=x->next;
    }
    
    node *y=head2;
    while(y!=NULL)
    {
        if(!isPresent(uni,y->data))
        {
            insLast(&uni,y->data);
        }
        y=y->next;
    }
    
    return uni;
}
int main()
{
    node *head1=NULL;
    node *head2=NULL;
    
    insLast(&head1,1);
    insLast(&head1,2);
    insLast(&head1,3);
    insLast(&head1,4);
    insLast(&head1,5);
    
    insLast(&head2,4);
    insLast(&head2,7);
    insLast(&head2,8);
    insLast(&head2,9);
    insLast(&head2,1);
    
    node *x=unionList(head1,head2);
    
    display(x);
    
}






