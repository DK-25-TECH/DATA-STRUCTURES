#include<stdio.h>
void swap(int *a,int *b)
{
    int x=*a;
    *a=*b;
    *b=x;
}
void merge(int a[],int l,int mid,int r)
{
    int x[r-l];
    int k=0;
    int i=l;
    int j=mid+1;
    while(i<=mid && j<=r)
    {
        if(a[i]<=a[j])
        {
            x[k++]=a[i++];
        }
        else
        {
            x[k++]=a[j++];
        }
    }
    while(i<=mid)
    {
        x[k++]=a[i++];
    }
    while(j<=r)
    {
        x[k++]=a[j++];
    }
    for(int i=0;i<k;i++)
    {
        a[i+l]=x[i];
    }
}
void mergesort(int a[],int l,int r)
{
    if(l<r){
    int mid=l+(r-l)/2;
    mergesort(a,l,mid);
    mergesort(a,mid+1,r);
    merge(a,l,mid,r);
    }
}
void print(int a[],int n)
{
    for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    print(a,n);
    printf("\n");
    mergesort(a,0,n-1);
    printf("\n");
    print(a,n);
    
}
