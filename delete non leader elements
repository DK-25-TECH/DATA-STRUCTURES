
#include<stdio.h>
#include<stdlib.h>
typedef struct node
{
    int data;
    struct node *next;
}*node;

node head=NULL;
node createNode(int data)
{
    node nn=malloc(sizeof(struct node));
    nn->data=data;
    nn->next=NULL;
    return nn;
}
void insertBeg(int data)
{
    node nn=createNode(data);
    if(head==NULL)
    {
        head=nn;
        return;
    }
    nn->next=head;
    head=nn;
}
void display()
{
    if(head==NULL)
    {
        return;
    }
    node tp=head;
    while(tp!=NULL)
    {
        printf("%d ",tp->data);
        tp=tp->next;
    }
}
void removee()
{
  node i=head,j,prv=NULL;
  while(i!=NULL)
  {
          int x=0;
      for(j=i->next;j!=NULL;j=j->next)
      {
          if(j->data>i->data)
          {
              x=1;
              break;
          }
      } 
      if(x)
      {
          node tp=i;
          if(i==head)
          {
              head=head->next;
              i=head;
          }
          else
          {
              prv->next=i->next;*/
              i=prv->next;
          }
          free(tp);
      }
      else
      {
          prv=i;
          i=i->next;
      }
  }
}

int main()
{  
    insertBeg(20);
    insertBeg(2010);
    insertBeg(250);
    insertBeg(2070);
    insertBeg(230);
    insertBeg(2020);
    insertBeg(200);
    insertBeg(3420);
    display();
    printf("\n");
    removee();
    display();
}

