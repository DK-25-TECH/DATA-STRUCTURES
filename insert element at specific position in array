#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    int x,pos;
    scanf("%d",&pos);
    scanf("%d",&x);
    
    if(pos<1 || pos>n+1)
    {
        printf("invalid");
    }
    
    for(int i=n;i>=pos;i--)
    {
        a[i]=a[i-1];
    }
    a[pos]=x;
    n++;
    for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
    
}
