/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     struct ListNode *next;
 * };
 */
struct ListNode* swapNodes(struct ListNode* head, int k) {
    struct ListNode *tp=head;
    int c=1;
    while(tp!=NULL)
    {
        c++;
        tp=tp->next;
    }
    tp=head;
    struct ListNode *first=head;
    struct ListNode *last=head;
    int lpos=c-k;
    int fp=1;
    int lp=1;
     
        while(fp<k)
        {
            first=first->next;
            fp++;
        }
        while(lp<lpos)
        {
            last=last->next;
            lp++;
        }
    int x=first->val;
    first->val=last->val;
    last->val=x;
    return head;
}
