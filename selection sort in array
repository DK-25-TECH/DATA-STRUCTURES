
#include <stdio.h>
void display(int a[],int n)
{
    for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
}
void sort(int a[],int n)
{
    for(int i=0;i<n-1;i++)
    {
       int x=i;
       for(int j=i;j<n;j++)
       {
           if(a[j]<a[x])
           {
               x=j;
           }
       }
       if(x!=i)
       {
           int z=a[i];
           a[i]=a[x];
           a[x]=z;
       }
       
    }
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    display(a,n);
    printf("\n");
    sort(a,n);
    printf("\n");
    display(a,n);
    
    
}
