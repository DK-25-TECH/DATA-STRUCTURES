#include<stdio.h>

void sort(int a[],int n)
{
    for(int i=1;i<n;i++)
    {
        int key=a[i];
       int j=i- 1;
       while(j>=0 && key<a[j])
       {
           a[j+1]=a[j];
           j--;
       }
       a[j+1]=key;
    }
}
void print(int a[],int n)
{
    for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    print(a,n);
    printf("\n");
    sort(a,n);
    printf("\n");
    print(a,n);
    
}
