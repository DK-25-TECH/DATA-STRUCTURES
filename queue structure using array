#include<stdio.h>
#define n 5
int q[n];
int f=-1,r=-1;
void enqueue(int data)
{
    if(r==n-1)
    {
        printf("queue is full");
        return;
    }
    if(f==-1 && r==-1)
    {
        f=r=0;
        q[f]=data;
    }
    else
    {
        
    q[++r]=data;
    }
}
void dequeue()
{
    if(f==-1 ||f>r)
    {
        printf("queue is empty");
        return;
    }
   
    f++;
    
    if(f>r)
    {
        f=r=-1;
    }
}
void display()
{
    for(int i=f;i<=r;i++)
    {
        printf("%d ",q[i]);
    }
}

int main()
{
    enqueue(10);
    enqueue(10);
    enqueue(10);
    enqueue(10);
    enqueue(10);
    dequeue();
    display();
}







