#include<stdio.h>
#include<stdlib.h>
typedef struct node
{
    int data;
    struct node *next;
}*node;

node head=NULL;
node createNode(int data)
{
    node nn=malloc(sizeof(struct node));
    nn->data=data;
    nn->next=NULL;
    return nn;
}
void insertBeg(int data)
{
    node nn=createNode(data);
    if(head==NULL)
    {
        head=nn;
        return;
    }
    nn->next=head;
    head=nn;
}
node nfromlst(int n)
{
    if(head==NULL || n<0)
    {
        return NULL;
    }
    node f=head;
    node s=head;
    int c=0;
    while(c<n)
    {
        if(f==NULL)
        {
            return NULL;
        }
        f=f->next;
        c++;
    }
    while(f!=NULL)
    {
        f=f->next;
        s=s->next;
    }
    return s;
}
int main()
{
    insertBeg(10);
    insertBeg(40);
    insertBeg(220);
    insertBeg(980);
    insertBeg(50);
    insertBeg(3320);
    node x=nfromlst(3);
    printf("%d",x->data);
}
